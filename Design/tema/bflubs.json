{
  "name": "BFLubs Corporate",
  "foreground": "#2F2F2F",
  "background": "#F4F6F8",
  "tableAccent": "#0B3C5D",

  "dataColors": [
    "#0B3C5D",
    "#1F6AA5",
    "#2E7D32",
    "#C62828",
    "#9E9E9E",
    "#455A64",
    "#78909C"
  ],

  "visualStyles": {

    "*": {
      "*": {
        "title": [
          {
            "show": true,
            "fontFamily": "Segoe UI",
            "fontSize": 12,
            "fontWeight": "SemiBold",
            "color": "#0B3C5D"
          }
        ],
        "background": [
          {
            "color": "#F4F6F8",
            "transparency": 0
          }
        ],
        "border": [
          {
            "show": false
          }
        ],
        "visualHeader": [
          {
            "foreground": "#0B3C5D",
            "background": "#F4F6F8",
            "border": "#E0E0E0"
          }
        ]
      }
    },

    "card": {
      "*": {
        "labels": [
          {
            "color": "#2F2F2F",
            "fontSize": 14,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryLabels": [
          {
            "color": "#455A64",
            "fontSize": 11
          }
        ],
        "background": [
          {
            "color": "#FFFFFF",
            "transparency": 0
          }
        ]
      }
    },

    "table": {
      "*": {
        "grid": [
          {
            "gridVertical": true,
            "gridVerticalColor": "#E0E0E0",
            "gridHorizontal": true,
            "gridHorizontalColor": "#E0E0E0"
          }
        ],
        "columnHeaders": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 11,
            "fontWeight": "SemiBold",
            "color": "#0B3C5D",
            "background": "#F4F6F8"
          }
        ],
        "values": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 10,
            "color": "#2F2F2F"
          }
        ]
      }
    },

    "matrix": {
      "*": {
        "rowHeaders": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 11,
            "color": "#0B3C5D"
          }
        ],
        "values": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 10,
            "color": "#2F2F2F"
          }
        ]
      }
    },

    "lineChart": {
      "*": {
        "dataLabels": [
          {
            "show": false
          }
        ],
        "legend": [
          {
            "show": true,
            "position": "Top",
            "fontFamily": "Segoe UI",
            "fontSize": 10
          }
        ]
      }
    },

    "barChart": {
      "*": {
        "legend": [
          {
            "show": true,
            "position": "Top"
          }
        ]
      }
    }
  }
}
