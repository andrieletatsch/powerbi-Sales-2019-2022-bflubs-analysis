{
  "name": "BFLubs Corporate",
  "foreground": "#2F2F2F",
  "background": "#F4F6F8",
  "tableAccent": "#0B3C5D",

  "dataColors": [
    "#0B3C5D",
    "#1F6AA5",
    "#2E7D32",
    "#ba4a44",
    "#9E9E9E",
    "#455A64",
    "#78909C"
  ],

  "visualStyles": {

    "*": {
      "*": {
        "title": [
          {
            "show": true,
            "fontFamily": "Segoe UI",
            "fontSize": 12,
            "fontWeight": "SemiBold",
            "color": {
              "solid": {
                "color": "#0B3C5D"
              }
            }
          }
        ],
        "background": [
          {
            "color": {
              "solid": {
                "color": "#F4F6F8"
              }
            },
            "transparency": 0
          }
        ]
      }
    },

    "card": {
      "*": {
        "background": [
          {
            "color": {
              "solid": {
                "color": "#FFFFFF"
              }
            },
            "transparency": 0
          }
        ],
        "labels": [
          {
            "color": {
              "solid": {
                "color": "#2F2F2F"
              }
            },
            "fontSize": 14,
            "fontFamily": "Segoe UI"
          }
        ],
        "categoryLabels": [
          {
            "color": {
              "solid": {
                "color": "#455A64"
              }
            },
            "fontSize": 11,
            "fontFamily": "Segoe UI"
          }
        ]
      }
    },

    "table": {
      "*": {
        "columnHeaders": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 11,
            "fontWeight": "SemiBold",
            "color": {
              "solid": {
                "color": "#0B3C5D"
              }
            }
          }
        ],
        "values": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 10,
            "color": {
              "solid": {
                "color": "#2F2F2F"
              }
            }
          }
        ]
      }
    },

    "matrix": {
      "*": {
        "rowHeaders": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 11,
            "color": {
              "solid": {
                "color": "#0B3C5D"
              }
            }
          }
        ],
        "values": [
          {
            "fontFamily": "Segoe UI",
            "fontSize": 10,
            "color": {
              "solid": {
                "color": "#2F2F2F"
              }
            }
          }
        ]
      }
    }
  }
}
